<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context 
		http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="stageShowService" class="com.system.service.impl.StageShowServiceImpl" />
	
	<bean id="propertyUtil" class="com.system.util.PropertyUtil" init-method="init"/>
	
	<bean id="showSystem" class="com.system.util.ShowSystem" init-method="init" depends-on="propertyUtil" />

</beans>